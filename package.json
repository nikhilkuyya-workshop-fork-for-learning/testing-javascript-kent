{
  "name": "my-testing-from-scratch",
  "version": "1.0.0",
  "description": "learn , build and play with testing",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build:dev": "webpack-dev-server",
    "mytest": "node --require ./01-fundamentals-testing/my-assertion-library.js",
    "lint": "eslint \"**/*.+(js)\"",
    "flow": "flow",
    "format": "npm run prettier -- --write ",
    "prettier": "prettier \"**/*.+(js|json)\"",
    "test": "jest",
    "validate": "npm run lint && npm run prettier -- --list-different && npm run flow && npm run test",
    "precommit": "lint-staged && npm run flow && npm run test"
  },
  "keywords": [
    "test-all"
  ],
  "author": "nikhilkuyya@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "eslint": "^5.8.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.85.0",
    "husky": "^1.1.3",
    "jest": "^23.6.0",
    "lint-staged": "^8.0.4",
    "prettier": "^1.15.1",
    "react": "^16.7.0-alpha.0",
    "react-dom": "^16.7.0-alpha.0",
    "react-testing-library": "^5.2.3",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
