{
  "name": "my-testing-from-scratch",
  "version": "1.0.0",
  "description": "learn , build and play with testing",
  "main": "index.js",
  "scripts": {
    "mytest": "node --require ./01-fundamentals-testing/my-assertion-library.js",
    "lint": "eslint \"**/*.+(js)\"",
    "flow": "flow",
    "format": "npm run prettier -- --write ",
    "prettier": "prettier \"**/*.+(js|json)\"",
    "validate": "npm run lint && npm run prettier -- --list-different && npm run flow",
    "precommit": "lint-staged && npm run flow"
  },
  "keywords": [
    "test-all"
  ],
  "author": "nikhilkuyya@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-config-prettier": "^3.1.0",
    "flow-bin": "^0.85.0",
    "husky": "^1.1.3",
    "lint-staged": "^8.0.4",
    "prettier": "^1.14.3"
  }
}
